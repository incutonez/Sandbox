{
  "extends": "eslint:recommended",
  "env": {
    "amd": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true,
    "worker": true
  },
  "rules": {
    "eqeqeq": 2,
    "comma-dangle": 1,
    "no-cond-assign": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-extra-semi": 1,
    "no-extra-parens": 1,
    "no-irregular-whitespace": 1,
    "no-redeclare": 1,
    "no-undef": 2,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false
      }
    ],
    "no-var": 0,
    "semi": 1,
    "semi-spacing": 1
  },
  "globals": {
    "Ext": true,
    "socket": true,
    "Routes": true,
    "Icons": true,
    "Styles": true,
    "UserProfile": true,
    "JefBox": true,
    "Enums": true
  }
}