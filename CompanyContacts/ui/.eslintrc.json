{
  "extends": [
    "@vue/typescript",
    "@vue/typescript/recommended",
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "rules": {
    "@typescript-eslint/ban-types": [
      "error",
      {
        "extendDefaults": true,
        "types": {
          "{}": false
        }
      }
    ],
    "@typescript-eslint/no-empty-interface": [
      "error",
      {
        "allowSingleExtends": true
      }
    ]
  }
}
