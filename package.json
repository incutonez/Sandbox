{
  "name": "@incutonez/sandbox",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "scripts": {
    "zelda:ui": "cd ZeldaWorldBuilder/ui && npm start",
    "companyContacts": "cd CompanyContacts && npm start",
    "rpan": "cd RPAN && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "updateDeps": "node ./updateDependencies.js",
    "lint": "npx eslint . --ext .js --fix",
    "lint:fix": "prettier --write && npx eslint --fix",
    "build:zelda": "cd ZeldaWorldBuilder/ui && npm run build",
    "explode": "npm cache clean --force && npx rimraf package-lock.json node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incutonez/Sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/incutonez/applications/issues"
  },
  "homepage": "https://github.com/incutonez/Sandbox#readme",
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1"
  },
  "workspaces": [
    "ZeldaWorldBuilder/ui",
    "CompanyContacts/api",
    "CompanyContacts/shared",
    "CompanyContacts/ui",
    "RPAN",
    "Differ/api",
    "Differ/shared",
    "Differ/ui",
    "TipTap",
    "Modeling",
    "PrimeVue"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
